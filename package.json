{
  "name": "amazona",
  "version": "1.0.0",
  "description": "Amazon clone application with ReactJS and NodeJS",
  "main": "server.js",
  "scripts": {
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel backend -d build",
    "clean": "rm -rf build && mkdir build",
    "start": "npm run build && node ./build/server.js",
    "dev": "nodemon --watch backend --exec babel-node backend/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MajemiteJames/amazon_react_clone.git"
  },
  "keywords": [
    "Amazon",
    "clone"
  ],
  "author": "Majemite Okoro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MajemiteJames/amazon_react_clone/issues"
  },
  "homepage": "https://github.com/MajemiteJames/amazon_react_clone#readme",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.11.5",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.26"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "nodemon": "^2.0.4"
  }
}
